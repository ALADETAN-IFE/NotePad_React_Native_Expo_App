{
  "expo": {
    "name": "Notepad",
    "slug": "notepad",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/logo1024x1024.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ifecodes.notepad",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo1024x1024.png",
        "monochromeImage": "./assets/images/logo1024x1024.png",
        "backgroundColor": "#00000000"
      },
      "package": "com.ifecodes.notepad"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo1024x1024.png",
          "imageWidth": 230,
          "resizeMode": "contain",
          "backgroundColor": "#64748b"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9d42b165-6446-4fd4-a26c-515c7350f76d"
      }
    },
    "owner": "ifecodes",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/9d42b165-6446-4fd4-a26c-515c7350f76d"
    }
  }
}
